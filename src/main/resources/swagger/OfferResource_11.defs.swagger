              $ref: '#/definitions/ProductID'
            maxItems: 100
            minItems: 1
          Price:
            description: 'Assigned Type: c-1100:Price'
            $ref: '#/definitions/Price'
          TermsAndConditionsFull:
            type: array
            items:
              $ref: '#/definitions/TermsAndConditionsFullID'
            maxItems: 100
            minItems: 1
          passiveOfferInd:
            description: If true, the Offer is passive for booking purposes.
            type: boolean
          ExtensionPoint:
            $ref: '#/definitions/ExtensionPoint'
        required:
          - Product
          - Price
          - TermsAndConditionsFull
  OfferSummary:
    allOf:
      - $ref: '#/definitions/OfferID'
      - properties:
          parentOfferRef:
            description: A reference to the Offer this offer is sold in conjunction with
            type: string
          Product:
            type: array
            items:
              $ref: '#/definitions/ProductID'
            maxItems: 100
            minItems: 1
          Price:
