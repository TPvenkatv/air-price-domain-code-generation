        type: string
        maxLength: 128
      OfferIdentifier:
        type: array
        items:
          $ref: '#/definitions/OfferIdentifier'
        maxItems: 1000
        minItems: 1
      ProductIdentifier:
        type: array
        items:
          $ref: '#/definitions/ProductIdentifier'
        maxItems: 10
      SegmentSequence:
        description: The segmentSequence within the product the action is being requested for. Used when multiple exist within a product
        type: integer
        format: int32
      ExtensionPoint:
        $ref: '#/definitions/ExtensionPoint'
