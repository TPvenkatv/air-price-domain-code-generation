            maxItems: 100
            minItems: 1
          passiveOfferInd:
            description: If true, the Offer is passive for booking purposes.
            type: boolean
          ExtensionPoint:
            $ref: '#/definitions/ExtensionPoint'
        required:
          - Product
          - Price
          - TermsAndConditionsFull
