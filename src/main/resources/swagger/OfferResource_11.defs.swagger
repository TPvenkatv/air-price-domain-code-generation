definitions:
  OfferListResponse:
    properties:
      '@type':
        type: string
      Offer:
        type: array
        items:
          $ref: '#/definitions/OfferID'
        maxItems: 1000
        minItems: 1
      transactionId:
        description: Unique transaction, correlation or tracking id for a single request and reply i.e. for a single transaction. Should be a 128 bit GUID format. Also know as E2ETrackingId.
        type: string
      traceId:
        description: Optional ID for internal child transactions created for processing a single request (single transaction). Should be a 128 bit GUID format. Also known as ChildTrackingId.
        type: string
      Result:
        description: 'Assigned Type: c-1100:Result'
        $ref: '#/definitions/Result'
      Identifier:
        description: 'Assigned Type: c-1100:Identifier'
        $ref: '#/definitions/Identifier'
      NextSteps:
        description: 'Assigned Type: c-1100:NextSteps'
        $ref: '#/definitions/NextSteps'
      ReferenceList:
        type: array
        items:
          $ref: '#/definitions/ReferenceList'
        maxItems: 5
      CurrencyRateConversion:
        type: array
        items:
          $ref: '#/definitions/CurrencyRateConversion'
        maxItems: 5
      SupplementalInformation:
        type: array
        items:
          $ref: '#/definitions/SupplementalInformation'
      DiagnosticResponse:
        type: array
        items:
          $ref: '#/definitions/NameValuePair'
        maxItems: 20
      ExtensionPoint:
        $ref: '#/definitions/ExtensionPoint'
    required:
      - '@type'
      - Offer
    discriminator: '@type'
  ErrorResponse:
    properties:
      '@type':
        type: string
      transactionId:
        description: Unique transaction, correlation or tracking id for a single request and reply i.e. for a single transaction. Should be a 128 bit GUID format. Also know as E2ETrackingId.
        type: string
      traceId:
        description: Optional ID for internal child transactions created for processing a single request (single transaction). Should be a 128 bit GUID format. Also known as ChildTrackingId.
        type: string
      Result:
        description: 'Assigned Type: c-1100:Result'
        $ref: '#/definitions/Result'
      Identifier:
        description: 'Assigned Type: c-1100:Identifier'
        $ref: '#/definitions/Identifier'
      NextSteps:
        description: 'Assigned Type: c-1100:NextSteps'
        $ref: '#/definitions/NextSteps'
      ReferenceList:
        type: array
        items:
          $ref: '#/definitions/ReferenceList'
        maxItems: 5
      CurrencyRateConversion:
        type: array
        items:
          $ref: '#/definitions/CurrencyRateConversion'
        maxItems: 5
      SupplementalInformation:
        type: array
        items:
          $ref: '#/definitions/SupplementalInformation'
      DiagnosticResponse:
